.header {
  margin: 0 30rpx;
  padding: 30rpx 0;
  border-bottom: 2px solid #eee;
}

.header > text {
  display: block;
  font-size: 36rpx;
}

.header > text:last-child {
  font-size: 26rpx;
  color: #aaa;
}

.tips {
  padding: 30rpx 120rpx;
  font-size: 26rpx;
  color: #aaa;
  /* background: #eee; */
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.seatWin {
  margin-top: 20rpx;
  padding: 30rpx 0;
  padding-right: 30rpx;
  display: flex;
  flex-direction: column;
  background: #eee;
}

.trNum {
  text-align: right;
  margin-right: 10rpx;
  display: inline-block;
  width: 40rpx;
  color: #999;
  position: relative;
  top: -6rpx;
}

.icon {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 8rpx;
  background: #fff;
  position: relative;
  top: 8rpx;
}

.none {
  background: #f13e22;
}

.select {
  background: #0a0;
}

.tr {
  text-align: center;
}

.tr > view {
  margin: 6rpx;
}

.enter {
  width: 100%;
  position: fixed;
  right: 0;
  bottom: 0;
}

.enterBtn {
  width: 100%;
  padding: 30rpx 0;
  text-align: center;
  color: #fff;
  background: #f13e22;
}

.price {
  color: orange;
  font-size: 46rpx;
  padding: 10rpx 30rpx;
  text-align: right;
}
